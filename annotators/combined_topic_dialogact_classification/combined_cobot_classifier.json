{
  "chainer": {
    "in": [
      "history","phrase"
    ],
    "in_y": [
      "y"
    ],
    "pipe": [
      {
        "class_name": "bert_preprocessor",
        "vocab_file": "{MODEL_PATH}/vocab.txt",
        "do_lower_case": false,
        "max_seq_length": 64,
        "in": [
          "history","phrase"
        ],
        "out": [
          "bert_features"
        ]
      },
      {
        "class_name": "bert_float_classifier",
        "n_classes": 44,
        "return_probas": true,
        "one_hot_labels": true,
        "multilabel": true,
        "bert_config_file": "{MODEL_PATH}/bert_config.json",
        "save_path": "{MODEL_PATH}/{MODEL_NAME}",
        "load_path": "{MODEL_PATH}/{MODEL_NAME}",
        "keep_prob": 0.5,
        "learning_rate": 1e-05,
        "learning_rate_drop_patience": 5,
        "learning_rate_drop_div": 2.0,
        "in": [
          "bert_features"
        ],
        "in_y": [
          "y"
        ],
        "out": [
          "y_pred_probas"
        ]
      }
    ],
    "out": [
      "y_pred_probas"
    ]
  },
  "metadata": {
    "variables": {
        "ROOT_PATH": "~/.deeppavlov",
        "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
        "MODEL_PATH": "/tmp",
        "MODEL_NAME": "model"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/tf.txt",
      "{DEEPPAVLOV_PATH}/requirements/bert_dp.txt"
    ],
    "imports": ["bert_float_classifier"]
  }
}
